<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="position">
        <enum name="left_top" value="0" />
        <enum name="right_top" value="1" />
        <enum name="right_bottom" value="2" />
        <enum name="left_bottom" value="3" />
    </attr>
    <attr name="radius" format="dimension"/>

    <declare-styleable name="ArcMenu">
        <attr name="position" />
        <attr name="radius"/>
    </declare-styleable>



    <attr name="menu_position" format="integer" >
        <enum name="left_top" value="0" />
        <enum name="right_top" value="1" />
        <enum name="right_bottom" value="2" />
        <enum name="left_bottom" value="3" />
    </attr>

    <declare-styleable name="MoonMenu">
        <attr name="menu_position" />
    </declare-styleable>

    <declare-styleable name="app">
        <attr name="bottomDragVisibleHeight" format="dimension" />
        <attr name="bototmExtraIndicatorHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ShadowLayout">
        <attr name="sl_shadowed" format="boolean" />
        <attr name="sl_shadow_distance" format="dimension" />
        <attr name="sl_shadow_angle" format="integer" />
        <attr name="sl_shadow_radius" format="dimension" />
        <attr name="sl_shadow_color" format="color" />
    </declare-styleable>
</resources>